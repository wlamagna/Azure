# Script used to get keys or set them in AKS

az keyvault secret show --name "DatabricksCluster-tatata" --vault-name "EUWRESOURCENAMEAKV001" | Set-Clipboard
$meme=az keyvault secret show --name "DatabricksCluster-tatata" --vault-name "EUWRESOURCENAMEAKV001" | Select-String "value"
$meme -replace '.*value": "(.*)"','$1' | Set-Clipboard

# Get login from a VM
$meme=az keyvault secret show --name 'Domainadminpassword' --vault-name EUWRESOURCEAKV01 --query 'value' -o tsv

# When i need to set a key:
az keyvault secret set --name MDResource01 --vault-name EUWRESOURCEAKV01 --value "https://tata" 
